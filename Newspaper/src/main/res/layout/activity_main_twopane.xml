<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:baselineAligned="false"
		android:divider="?android:attr/dividerHorizontal"
		android:orientation="horizontal"
		android:showDividers="middle"
		tools:context="dh.newspaper.MainActivity">

	<fragment
			android:id="@+id/fragment_tags"
			android:name="dh.newspaper.view.TagsFragment"
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:layout_weight="1"
			tools:layout="@layout/fragment_tags"/>

	<fragment
			android:id="@+id/fragment_feeds"
			android:name="dh.newspaper.view.FeedsFragment"
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:layout_weight="3"
			tools:layout="@layout/fragment_feeds"/>

	<!--<fragment
			android:id="@+id/fragment_tags"
			android:name="dh.newspaper.view.TagsFragment"
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:layout_weight="1"
			tools:layout="@layout/fragment_tags"/>

	<fragment
			android:id="@+id/fragment_feeds"
			android:name="dh.newspaper.view.FeedsFragment"
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:layout_marginLeft="8dp"
			android:layout_weight="3"
			tools:layout="@layout/fragment_feeds"/>-->

</LinearLayout>
